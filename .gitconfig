[alias]
	co = checkout
  br = branch
  ci = commit 
  aa = add .
  st = status
  unstage = reset HEAD --
  ps = push
  pl = pull
  lgp = log --pretty=oneline
  r = remote
  lgg = log --graph --pretty=format:'%C(green)%h%Creset  %s'
  l = log --pretty=format:'* %C(green)%h%Creset %an - %s'
  psod = push origin development
  ft = fetch
  mod = merge origin development
[core]
  editor = vim

[color]
  branch = auto
  diff = auto
  status = auto
